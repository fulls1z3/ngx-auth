{
  "$schema": "../../../node_modules/ng-packagr/package.schema.json",
  "name": "@ngx-auth/auth0",
  "version": "9.0.0",
  "description": "Auth0 platform implementation of ngx-auth",
  "repository": {
    "type": "git",
    "url": "https://github.com/fulls1z3/ngx-auth.git"
  },
  "keywords": [
    "security",
    "authentication",
    "server side rendering",
    "jwt",
    "auth0",
    "auth",
    "universal",
    "angular"
  ],
  "author": {
    "name": "Burak Tasci",
    "email": "mail@buraktasci.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fulls1z3/ngx-auth/issues"
  },
  "homepage": "https://github.com/fulls1z3/ngx-auth/tree/master/packages/@ngx-auth/auth0#readme",
  "peerDependencies": {
    "@angular/common": ">=10.0.0 <11.0.0",
    "@angular/core": ">=10.0.0 <11.0.0",
    "@angular/platform-browser-dynamic": ">=10.0.0 <11.0.0",
    "@angular/router": ">=10.0.0 <11.0.0",
    "@ngx-auth/core": ">=10.0.0 <11.0.0",
    "auth0-js": ">=9.0.0",
    "rxjs": ">=6.0.0"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts",
      "umdModuleIds": {
        "rxjs": "rxjs",
        "auth0-js": "auth0js",
        "@ngx-auth/core": "ngxAuth_core"
      }
    },
    "dest": "../../../dist/@ngx-auth/auth0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "../../../dist/@ngx-auth/core"
  }
}
